{
  "name": "effect-ui",
  "version": "0.0.1",
  "description": "A reactive UI framework built on Effect.ts primitives",
  "type": "module",
  "license": "MIT",
  "author": "Jon Laing",
  "repository": {
    "type": "git",
    "url": "https://github.com/jonlaing/effect-ui.git",
    "directory": "packages/effect-ui"
  },
  "keywords": [
    "effect",
    "effect-ts",
    "ui",
    "reactive",
    "signals",
    "frontend",
    "framework"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "effect": "^3.0.0"
  }
}
