{
  "name": "effex-monorepo",
  "version": "0.0.1",
  "private": true,
  "description": "A reactive UI framework built on Effect.ts primitives",
  "type": "module",
  "packageManager": "pnpm@10.14.0",
  "license": "MIT",
  "author": "Jon Laing",
  "repository": {
    "type": "git",
    "url": "https://github.com/jonlaing/effex.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "pnpm -r build",
    "preview": "vite preview",
    "lint": "eslint .",
    "format": "prettier --write .",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "bench": "vitest bench",
    "typecheck": "tsc --noEmit",
    "storybook": "storybook dev -p 6006 -c packages/primitives/.storybook",
    "build-storybook": "storybook build -c packages/primitives/.storybook",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm build && changeset publish",
    "docs:gen": "rm -rf docs && typedoc"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@chromatic-com/storybook": "^4.1.3",
    "@eslint/js": "^9.39.1",
    "@eslint/json": "^0.14.0",
    "@storybook/addon-a11y": "^10.1.8",
    "@storybook/addon-docs": "^10.1.8",
    "@storybook/addon-storysource": "^8.6.14",
    "@storybook/addon-vitest": "^10.1.8",
    "@storybook/html-vite": "^10.1.8",
    "@types/node": "^24.10.1",
    "@vitest/browser-playwright": "^4.0.15",
    "@vitest/coverage-v8": "^4.0.15",
    "effect": "^3.19.9",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-storybook": "^10.1.8",
    "globals": "^16.5.0",
    "jiti": "^2.6.1",
    "jsdom": "^27.2.0",
    "playwright": "^1.57.0",
    "prettier": "^3.7.4",
    "storybook": "^10.1.8",
    "tsup": "^8.0.0",
    "typedoc": "^0.28.15",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.1",
    "vite": "^7.2.4",
    "vitest": "^4.0.15"
  }
}
